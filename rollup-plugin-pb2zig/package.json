  "bugs": {
    "url": "https://github.com/chung-leong/pb2zig/issues"
  },
  "homepage": "https://github.com/chung-leong/pb2zig#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "c8": "^8.0.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "mocha-skip-if": "^1.0.4",
    "rollup": "^3.23.0"
  }
